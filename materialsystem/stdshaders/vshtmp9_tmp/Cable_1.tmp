; T = baseTexture																								; LINEINFO(Cable.vsh)(13)
; N = particle normal (stored in the normal map)																								; LINEINFO(Cable.vsh)(14)
; L = directional light direction																								; LINEINFO(Cable.vsh)(15)
;																								; LINEINFO(Cable.vsh)(16)
